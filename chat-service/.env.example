#### Chat service environment example
# Copy to chat-service/.env and set real values before starting

# MongoDB connection string (used by chat-service)
MONGO_URI=mongodb://localhost:27017/chatdb

# HTTP port
PORT=8082

# JWT secret used for signing tokens (use a strong secret in production)
JWT_SECRET=changeme

# RabbitMQ / AMQP URL used for inter-service messaging
MESSAGE_BROKER_URL=amqp://localhost

# Node environment
NODE_ENV=development
